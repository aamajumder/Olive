{
  "input_model": {
    "type": "PyTorchModel",
    "config": {
      "model_path": "tiiuae/falcon-7b",
      "model_loader": "load_pytorch_origin_model",
      "model_script": "user_script.py",
      "io_config": {
        "input_names": [ "input_ids" ],
        "output_names": [ "output" ],
        "dynamic_axes": {
          "input_ids": {
            "0": "batch",
            "1": "sequence"
          }
        }
      },
      "dummy_inputs_func": "dummy_inputs"
    }
  },
  "systems": {
    "local_system": {
      "type": "LocalSystem",
      "config": {
        "accelerators": [ "gpu" ]
      }
    }
  },
  "evaluators": {
    "common_evaluator": {
      "metrics": [
        {
          "name": "latency",
          "type": "latency",
          "sub_types": [
            {
              "name": "avg",
              "priority": 1
            },
            { "name": "max" },
            { "name": "min" }
          ],
          "user_config": {
            "user_script": "user_script.py",
            "dataloader_func": "create_dataloader",
            "batch_size": 1
          }
        }
      ]
    }
  },
